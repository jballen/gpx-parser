{
  "name": "gpx-parser-ts",
  "version": "1.0.1",
  "description": "GPX Parser written in Typescript",
  "main": "./dist/main",
  "files": [
    "dist",
    "src",
    "esm"
  ],
  "scripts": {
    "clean": "rimraf dist esm",
    "prebuild": "npm run clean",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "jest"
  },
  "keywords": [
    "gpx",
    "gps",
    "typescript"
  ],
  "author": "Brock Allen",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "jest": "^29.4.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/xml2js": "^0.4.11",
    "xml2js": "^0.4.23"
  }
}
